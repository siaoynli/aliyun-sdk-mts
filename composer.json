{
    "name": "siaoynli/aliyun-sdk-mts",
    "type": "library",
    "description": "aliyun-php-sdk-mts 封装，支持composer方式使用",
    "keywords": [
      "aliyun",
      "AliCloud",
      "sdk",
      "mts"
    ],
    "homepage": "https://github.com/siaoynli/aliyun-sdk-mts",
    "license": "MIT",
    "authors": [
      {
        "name": "siaoynli",
        "email": "siaoynli@126.com",
        "homepage": "https://github.com/siaoynli"
      }
    ],
    "require": {
      "php": ">=5.4",
      "siaoynli/aliyun-sdk-core": "^1.0"
    },
    "require-dev": {
      "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
      "psr-4": {
        "AliCloud\\Mts\\": "src/Mts/Request/V20140618"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "AliCloud\\Mts\\": "tests/"
      }
    },
	"minimum-stability": "dev"
}
